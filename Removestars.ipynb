   "source": [
    "class solution:\n",
    "    def Removestars(self,s:str)--> str:\n",
    "        stack = []\n",
    "        for i in s:\n",
    "            if i == '*':\n",
    "                stack.pop()\n",
    "            else:\n",
    "                stack.append(i)\n",
    "        return"
   ]
